{
  "name": "queue-manage",
  "version": "1.0.0",
  "type": "module",
  "description": "Boilerplate for React + Express SSR with separate admin and user dashboards",
  "main": "server/index.js",
  "scripts": {
    "build:admin": "webpack --config webpack.admin.config.cjs",
    "build:user": "webpack --config webpack.user.config.cjs",
    "build": "npm run build:admin && npm run build:user && node scripts/copyPublic.js",
    "start": "node server/index.js",
    "dev": "nodemon --watch server --exec babel-node server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "nodemon": "^2.0.22",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "vitest": "^1.0.0",
    "supertest": "^6.3.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jsdom": "^22.1.0"
  }
}

